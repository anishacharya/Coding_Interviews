#include <iostream>
using namespace std;

int gcd(int x,int y)
{
	if(x==0)
		return y;
	else
		return gcd(x,y%x);
}
int main()
{
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	//int x=0;
	cout<<gcd(10,15)<<endl;
	return 0;
}
