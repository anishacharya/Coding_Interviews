#include <iostream>
using namespace std;

int square(int p)
{
	int x = p*p
			+ p*p;
	return x;
}
int main()
{
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	int x=0;

	cout<<square(10)<<endl;


	return 0;
}
